<div id="login-container">
  <%= form_for :login do |f| %>

    <%= f.text_field :email, placeholder:"Email Address" %>

    <%= f.password_field :password, placeholder:"Password" %>

    <%= f.submit 'Log In' %>

  <% end %>
  <p>Don't have an account? <%= link_to "Signup", signup_path %></p>
</div>
